<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans default-lazy-init="false">
	<bean id="queryAction" class="com.best1.report.action.QueryAction" scope="prototype">
		<property name="queryService">
			<ref bean="queryService"/>
		</property>
	</bean>
	
	<bean id="exportAuditAction" class="com.best1.report.action.ExportAuditAction" scope="prototype">
		<property name="exportAuditService">
			<ref bean="exportAuditService"/>
		</property>
	</bean>
	
	<bean id="productStatAction" class="com.best1.report.action.ProductStatAction" scope="prototype">
		<property name="productStatService">
			<ref bean="productStatService"/>
		</property>
	</bean>
	
	<bean id="channelStatAction" class="com.best1.report.action.ChannelStatAction" scope="prototype">
		<property name="channelStatService">
			<ref bean="channelStatService"/>
		</property>
	</bean>
	
	<bean id="assessStatAction" class="com.best1.report.action.AssessStatAction" scope="prototype">
		<property name="assessStatService">
			<ref bean="assessStatService"/>
		</property>
	</bean>
	
	<bean id="detailStatAction" class="com.best1.report.action.DetailStatAction" scope="prototype">
		<property name="detailStatService">
			<ref bean="detailStatService"/>
		</property>
	</bean>
	
	<bean id="customerStatAction" class="com.best1.report.action.CustomerStatAction" scope="prototype">
		<property name="customerStatService">
			<ref bean="customerStatService"/>
		</property>
	</bean>
	
	<bean id="campainStatAction" class="com.best1.report.action.CampainStatAction" scope="prototype">
		<property name="campainStatService">
			<ref bean="campainStatService"/>
		</property>
	</bean>
	
	<bean id="summaryStatAction" class="com.best1.report.action.SummaryStatAction" scope="prototype">
		<property name="summaryStatService">
			<ref bean="summaryStatService"/>
		</property>
	</bean>
	
	<bean id="financeSaleDetailsAction" class="com.best1.report.action.FinanceSaleDetailsAction" scope="prototype">
		<property name="financeService">
			<ref bean="financeService"/>
		</property>
	</bean>
	
</beans>