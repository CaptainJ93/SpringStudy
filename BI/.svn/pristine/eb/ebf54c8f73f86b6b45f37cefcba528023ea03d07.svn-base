/* 
 *	表格 指定行列冻结样式
 */

.freezTable{
	border-collapse: collapse;
	border: 1px solid #666666;
}
.freezTable th{
	height: 30px;
	/*width: 70px;*/
	BORDER-BOTTOM: #666666 1px solid;
	TEXT-ALIGN: center;
	BORDER-LEFT: #666666 1px solid;
	BACKGROUND-COLOR: #d2e0f1;
	FONT-FAMILY: 宋体;
	COLOR: black;
	FONT-SIZE: 9pt;
	VERTICAL-ALIGN: middle;
	BORDER-TOP: #666666 1px solid;
	FONT-WEIGHT: bold;
	BORDER-RIGHT: #666666 1px solid;
}

.freezTable td {
	height: 20px;
	/*width: 70px;*/
	BORDER-BOTTOM: #666666 1px solid;
	TEXT-ALIGN: center;
	BORDER-LEFT: #666666 1px solid;
	BACKGROUND-COLOR: #eaf4fd;
	FONT-FAMILY: 宋体;
	COLOR: black;
	FONT-SIZE: 9pt;
	VERTICAL-ALIGN: middle;
	BORDER-TOP: #666666 1px solid;
	FONT-WEIGHT: normal;
	BORDER-RIGHT: #666666 1px solid;
}